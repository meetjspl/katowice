<header id="header">
  <a id="header-logo" href="<%- url_for("/") %>">
  <% if (theme.customize.logo && theme.customize.logo.enabled) { %>
    <div id="logo" style="background-image: url(<%- url_for(theme.customize.logo.url) %>);"></div>
  <% } %>
    <div id="title">
      <h1><%= config.subtitle %></h1>
    </div>
  </a>
  <div id="nav">
    <ul>
      <li class="icon">
        <a href="#"><i class="fa fa-bars fa-2x"></i></a>
      </li>
      <% for (var i in config.nav) { %> 
        <li><a href="<%- url_for(config.nav[i]) %>"><%= i %></a></li>
      <% } %>
    </ul>
  </div>
</header>
