{
  "name": "meetjs-katowice",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "6.2.0"
  },
  "scripts": {
    "validate": "npx ajv -s speakers/list.schema.json -d speakers/list.json",
    "develop": "npx hexo server",
    "gen:pages": "npx hexo generate -f",
    "gen:pages:watch": "npx hexo generate -fw",
    "gen:speakers": "cd speakers && node generate.js",
    "build": "npm run gen:speakers && npx hexo generate"
  },
  "dependencies": {
    "ajv-cli": "^5.0.0",
    "ejs": "^3.1.8",
    "hexo": "^6.2.0",
    "hexo-cli": "^4.3.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-pagination": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^5.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-server": "^3.0.0"
  }
}
